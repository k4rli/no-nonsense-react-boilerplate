    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "browserslist": "4.4.1",
