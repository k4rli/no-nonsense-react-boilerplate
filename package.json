{
  "name": "no-nonsense-react-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate without the usual unnecessary stuff",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "precommit": "lint-staged"
